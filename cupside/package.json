{
  "name": "sba_team",
  "version": "1.0.0",
  "dependencies": {
    "@11ty/eleventy": "^0.11.0",
    "bootstrap": "^4.4.1",
    "eleventy-plugin-sass": "^1.0.0",
    "jquery": "^3.5.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.3.1"
  },
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "build": "run-s prod:*",
    "dev:eleventy": "eleventy --serve",
    "dev:parcel": "parcel watch ./src/_assets/js/main.js --out-dir ./dist/assets",
    "prod:eleventy": "eleventy --pathprefix=cup",
    "prod:parcel": "parcel build ./src/_assets/js/main.js --out-dir ./dist/assets"
  },
  "staticFiles": {
    "staticPath": [
      "./src/_assets/static"
    ],
    "watcherGlob": "**"
  }
}
